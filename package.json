{
  "name": "@ndlib/ndlib-cdk",
  "version": "1.3.1",
  "description": "Reusable CDK modules used within Hesburgh Libraries of Notre Dame",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "tslint --fix -p tsconfig.json && prettier --write \"@(src|tests)/**/*.@(ts|js|json|md|yml)\" \"*.@(ts|js|json|md|yml)\"",
    "lint": "tslint -p tsconfig.json && prettier --check \"@(src|tests)/**/*.@(ts|js|json|md|yml)\" \"*.@(ts|js|json|md|yml)\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git checkout -B bump-$npm_package_version && git add -A src",
    "postversion": "git push --set-upstream origin bump-$npm_package_version && git push --tags",
    "watch": "tsc-watch --onSuccess \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndlib/ndlib-cdk.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "ndlib"
  ],
  "author": {
    "name": "Hesburgh Libraries, University of Notre Dame",
    "url": "https://library.nd.edu"
  },
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/ndlib/ndlib-cdk/issues"
  },
  "homepage": "https://github.com/ndlib/ndlib-cdk#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@aws-cdk/assert": "^1.28.0",
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tsc-watch": "^4.2.8",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "@aws-cdk/aws-cloudformation": "^1.28.0",
    "@aws-cdk/aws-codepipeline": "^1.28.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.28.0",
    "@aws-cdk/aws-events-targets": "^1.28.0",
    "@aws-cdk/aws-cloudwatch": "^1.28.0",
    "@aws-cdk/aws-sns": "^1.28.0",
    "@aws-cdk/core": "^1.28.0"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.28.0",
    "@aws-cdk/aws-codepipeline": "^1.28.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.28.0",
    "@aws-cdk/aws-events-targets": "^1.28.0",
    "@aws-cdk/aws-cloudwatch": "^1.28.0",
    "@aws-cdk/aws-sns": "^1.28.0",
    "@aws-cdk/core": "^1.28.0"
  }
}
