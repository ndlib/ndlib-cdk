{
  "name": "@ndlib/ndlib-cdk",
  "version": "1.0.3",
  "description": "Reusable CDK modules used within Hesburgh Libraries of Notre Dame",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json && prettier --check \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git checkout -B bump-$npm_package_version && git add -A src",
    "postversion": "git push --set-upstream origin bump-$npm_package_version && git push --tags",
    "watch": "tsc-watch --onSuccess \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndlib/ndlib-cdk.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "ndlib"
  ],
  "author": {
    "name": "Hesburgh Libraries, University of Notre Dame",
    "url": "https://library.nd.edu"
  },
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/ndlib/ndlib-cdk/issues"
  },
  "homepage": "https://github.com/ndlib/ndlib-cdk#readme",
  "devDependencies": {
    "@aws-cdk/assert": "^1.11.0",
    "@types/jest": "^24.0.18",
    "aws-cdk": "^1.11.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tsc-watch": "^3.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.11.0",
    "@aws-cdk/core": "^1.11.0"
  }
}
